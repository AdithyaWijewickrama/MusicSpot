<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="520.0"
	prefWidth="450.0" xmlns="http://javafx.com/javafx/17"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.LoginController">
	<children>
		<Label alignment="CENTER" contentDisplay="CENTER" layoutX="15.0"
			layoutY="47.0" prefHeight="17.0" prefWidth="420.0" text="Music Spot"
			textAlignment="CENTER">
			<font>
				<Font size="36.0" />
			</font>
		</Label>
		<Label alignment="CENTER" contentDisplay="CENTER" layoutX="15.0"
			layoutY="126.0" prefHeight="17.0" prefWidth="420.0" text="User name">
			<font>
				<Font size="17.0" />
			</font>
		</Label>
		<TextField fx:id="usernameTxt" alignment="CENTER"
			layoutX="15.0" layoutY="162.0" onAction="#gotopassword"
			onKeyTyped="#resetWarnings" prefHeight="25.0" prefWidth="420.0">
			<font>
				<Font size="16.0" />
			</font>
		</TextField>
		<Label alignment="CENTER" contentDisplay="CENTER" layoutX="15.0"
			layoutY="213.0" prefHeight="26.0" prefWidth="420.0" text="Password">
			<font>
				<Font size="17.0" />
			</font>
		</Label>
		<PasswordField fx:id="passwordTxt" alignment="CENTER"
			layoutX="15.0" layoutY="248.0" onKeyTyped="#resetWarnings"
			prefHeight="25.0" prefWidth="420.0">
			<font>
				<Font size="16.0" />
			</font>
		</PasswordField>
		<Button fx:id="loginBtn" layoutX="111.0" layoutY="351.0"
			mnemonicParsing="false" onAction="#login" prefHeight="35.0"
			prefWidth="228.0" text="Login">
			<font>
				<Font size="16.0" />
			</font>
		</Button>
		<Button fx:id="signInBtn" layoutX="111.0" layoutY="426.0"
			mnemonicParsing="false" onAction="#switchToSignIn" prefHeight="35.0"
			prefWidth="228.0" text="Sign In">
			<font>
				<Font size="16.0" />
			</font>
		</Button>
		<Label alignment="CENTER" contentDisplay="CENTER" layoutX="15.0"
			layoutY="394.0" prefHeight="26.0" prefWidth="420.0"
			text="Don't have an account yet?">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<Label fx:id="required" alignment="CENTER"
			contentDisplay="CENTER" layoutX="15.0" layoutY="299.0"
			prefHeight="26.0" prefWidth="420.0" text="Required*"
			textFill="#d30f0f">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
